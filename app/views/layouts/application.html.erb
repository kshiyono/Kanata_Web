<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <div id="header_only"><navbar></navbar></div>
    <div class="container">
      <%= yield %>
    </div>
    <div id="footer_only"><navfoo></navfoo></div>
    <%= debug(params) if Rails.env.development? %>
  </body>
</html>
<%= javascript_pack_tag 'header_only' %>
<%= javascript_pack_tag 'footer_only' %>
